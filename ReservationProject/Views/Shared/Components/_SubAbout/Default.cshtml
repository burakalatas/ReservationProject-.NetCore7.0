@model List<SubAbout>

<section class="w3l-bottom py-5">
    <div class="container py-md-4 py-3 text-center">
        <div class="row my-lg-4 mt-4">
            <div class="col-lg-9 col-md-10 ml-auto">
                <div class="bottom-info ml-auto">
                    @foreach(var item in Model){                           
                    <div class="header-section text-left">
                        <h3 class="hny-title two">@item.SubAboutTitle</h3>
                        <p class="mt-3 pr-lg-5">
                            @item.SubAboutDescription
                        </p>
                        <a href="~/Traversal/about.html" class="btn btn-style btn-secondary mt-5">Rezervasyon Yap</a>
                    </div>
                    }
                </div>
            </div>
        </div>
    </div>
</section>